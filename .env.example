# Environment Configuration
ENVIRONMENT=development
DEBUG=True

# Server Configuration
HOST=0.0.0.0
PORT=8000

# CORS Settings
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_KEY=your-supabase-service-key  # Optional, for admin operations

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
REDIS_CACHE_TTL=3600

# KeyPick API Security
# 用于保护 KeyPick API 的密钥（Dify 调用时需要提供）
KEYPICK_API_KEYS=keypick-xxxxx-001,keypick-xxxxx-002  # 可以配置多个，逗号分隔


# MediaCrawler Configuration
MEDIACRAWLER_PATH=./MediaCrawler
MEDIACRAWLER_TIMEOUT=3600

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/keypick.log

# Task Configuration
MAX_CONCURRENT_TASKS=5
TASK_RESULT_TTL=86400

# Rate Limiting
RATE_LIMIT_ENABLED=True
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_PERIOD=60